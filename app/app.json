{
  "expo": {
    "name": "Baladeur",
    "slug": "baladeur",
    "version": "0.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.dekajoo.baladeur"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "scheme": "baladeur",
    "description": "Une application qui vous laisse vous perdre pour découvrir des endroits étonnants."
  }
}